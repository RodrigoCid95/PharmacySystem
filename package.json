{
  "name": "pharmacy-system",
  "productName": "Pharmacy System",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": "Rodrigo Cid",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "pharmacy_system"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devServer": {
              "hot": true,
              "historyApiFallback": true
            },
            "mainConfig": "./webpack.main.config.js",
            "nodeIntegration": true,
            "contextIsolation": true,
            "webSecurity": false,
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/login/index.html",
                  "js": "./src/login/renderer.tsx",
                  "preload": {
                    "js": "./src/login/API.ts"
                  },
                  "name": "login_window"
                },
                {
                  "html": "./src/admin/index.html",
                  "js": "./src/admin/renderer.tsx",
                  "preload": {
                    "js": "./src/admin/API/index.ts"
                  },
                  "name": "admin_window"
                },
                {
                  "html": "./src/checkout-box/index.html",
                  "js": "./src/checkout-box/renderer.tsx",
                  "preload": {
                    "js": "./src/checkout-box/API/index.ts"
                  },
                  "name": "checkout_box_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.61",
    "@types/archiver": "^5.3.1",
    "@types/cors": "^2.8.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/sqlite3": "^3.1.8",
    "@types/unzipper": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.5.1",
    "electron": "^16.0.6",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.25.4",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@fluentui/example-data": "^8.3.0",
    "@fluentui/react": "^8.49.1",
    "@fluentui/react-file-type-icons": "^8.5.9",
    "@fluentui/react-icons-mdl2": "^1.2.13",
    "@uifabric/icons": "^7.6.2",
    "archiver": "^5.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "file-loader": "^6.2.0",
    "jsbarcode": "^3.11.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "sqlite3": "^5.0.2",
    "unzipper": "^0.10.11"
  }
}
